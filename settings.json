{
  "editor.lineNumbers": "relative",
  "editor.minimap.showSlider": "always",
  "gitlens.gitCommands.skipConfirmations": ["fetch:command", "switch:command"],
  "editor.autoSurround": "languageDefined",
  "vim.handleKeys": {
    "<C-c>": false,
    "<C-v>": false,
    "<C-x>": false,
    "<C-p>": false,
    // "<C-k>": false,
    "<C-a>": false,
    "<C-w>": false,
    "<C-f>": false,
    "<C-y>": false,
    "<C-t>": false,
    "<C-e>": false,
    "<C-d>": false,
    "<C-u>": false,
    "<C-o>": false
  },
  "vim.leader": "<Space>",
  "vim.scroll": 0,
  "vim.easymotion": true,
  "vim.easymotionKeys": "asdfghjkl;iwen",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.statusBarColors.easymotioninputmode": "#ff0000",
  "vim.statusBarColors.easymotionmode": "#ff0000",
  // "editor.fontFamily": "",
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [">"],
      "commands": ["editor.action.addCommentLine"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.removeCommentLine"]
    },
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    // EDITOR COMMANDS
    {
      "before": ["E"],
      "after": ["$"]
    },
    {
      "before": ["B"],
      "after": ["0"]
    },
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    // {
    //   "before": ["<C-k>"],
    //   "after": [""]
    // },
    {
      "before": [">"],
      "commands": ["editor.action.addCommentLine"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.removeCommentLine"]
    },
    {
      "before": ["/"],
      "after": ["<Space>", "<Space>", "/"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["g", "h", "w"],
      "commands": ["editor.action.wordHighlight.prev"]
    },
    {
      "before": ["g", "l", "w"],
      "commands": ["editor.action.wordHighlight.next"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<leader>", "["],
      "commands": ["editor.action.jumpToBracket"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["editor.showCallHierarchy"]
    },
    {
      "before": ["<CR>"],
      "after": ["o", "<Esc>"]
    },
    {
      "before": ["g", "l", "p"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["g", "h", "p"],
      "commands": ["editor.action.marker.prev"]
    },

    // LEADER STUFF
    // {
    //     "before": ["<leader>", "f"],
    //     "commands": ["workbench.action.experimental.quickTextSearch"]
    // },
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.formatSelection"]
    }
    // {
    //     "before": ["k", "j"],
    //     "after": ["<Esc>"],
    // },
  ],
  "vim.highlightedyank.enable": true,
  "glassit.alpha": 250,
  "diffEditor.ignoreTrimWhitespace": false,
  "vim.cursorStylePerMode.visual": "underline-thin",
  "vim.cursorStylePerMode.insert": "line-thin",
  "terminal.integrated.env.windows": {},
  "console-ninja.featureSet": "Community",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "editor.minimap.enabled": false,
  "search.searchOnType": false,
  "breadcrumbs.icons": false,
  "breadcrumbs.showArrays": false,
  "breadcrumbs.symbolPath": "off",
  "multiCommand.commands": [
    {
      "command": "multiCommand.goToSymbolAndCategory",
      "sequence": [
        {
          "command": "workbench.action.gotoSymbol" // √ñppnar "Go to Symbol in File"
        },
        {
          "command": "type", // Simulerar att skriva ":"
          "args": {
            "text": ":"
          }
        }
      ]
    },
    {
      "command": "multiCommand.pageDownAndCenter",
      "sequence": [
        {
          "command": "editorScroll",
          "args": {
            "to": "down",
            "by": "line",
            "value": 15,
            "revealCursor": true
          }
        },
        {
          "command": "cursorMove",
          "args": {
            "to": "viewPortCenter"
          }
        }
      ]
    },
    {
      "command": "multiCommand.pageUpAndCenter",
      "sequence": [
        {
          "command": "editorScroll",
          "args": {
            "to": "up",
            "by": "line",
            "value": 15,
            "revealCursor": true
          }
        },
        {
          "command": "cursorMove",
          "args": {
            "to": "viewPortCenter"
          }
        }
      ]
    }
  ],
  "workbench.editor.showTabs": "single",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "window.title": "üê±‚Äçüë§${dirty}${activeEditorMedium}${separator}${rootName}${separator}${profileName}${separator}${remoteName}",
  "workbench.startupEditor": "none",
  "window.menuBarVisibility": "toggle",
  "debug.inlineValues": "on",
  "workbench.activityBar.location": "hidden",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sideBar.location": "right",
  "dotnet.server.useOmnisharp": true,
  "workbench.colorTheme": "Catppuccin Mocha",
  "debug.onTaskErrors": "debugAnyway",
  "dotnet-test-explorer.enableTelemetry": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.fontFamily": "Consolas",
  "editor.stickyScroll.enabled": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": -0.3,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.editorActionsLocation": "hidden",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.accessibilitySupport": "off",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.codeLens.enabled": false,
  "dotnet.inlayHints.enableInlayHintsForParameters": true,
  "files.exclude": {
    "**/bin": true,
    "**/obj": true
  },
  "dotnet.preferCSharpExtension": true,
  "workbench.tree.indent": 20,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
